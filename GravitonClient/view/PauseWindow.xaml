<Window x:Class="GravitonClient.view.PauseWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:GravitonClient.view"
        mc:Ignorable="d"
        Title="PauseWindow" Height="450" Width="800">
    <Grid>
        <Button x:Name="btnResume" Content="Resume" Click="Resume_Click"/>
        <Button x:Name="btnLoad" Content="Load" Click="BtnLoad_Click"/>
        <Button x:Name="Exit" Content="Save and Exit" Click="Exit_Click"/>
    </Grid>
</Window>
